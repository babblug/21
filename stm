PROGRAM:  #include<stdio.h>  
#include<conio.h> 
 void main()  {  int a[3][3],b[3][3],c[3][3],i,j,k,m,n,p,q;  printf("Enter 1st matrix no.of rows & cols:"); scanf("%d%d",&m,&n); printf("Enter 2nd matrix no.of rows & cols:"); scanf("%d%d",&p,&q); printf("\n enter  the 1 matrix elements\n");  for(i=0;i<m;i++) {  for(j=0;j<n;j++){  scanf("%d",&a[i][j]);  }  }  printf("\n enter the 2 matrix elements\n");  for(i=0;i<p;i++) {  for(j=0;j<q;j++) {  scanf("%d",&b[i][j]);  }  }   //multiplication for(i=0;i<m;i++)  {  for(j=0;j<q;j++)  {  c[i][j]=0;  for(k=0;k<n;k++)  {  c[i][j]=c[i][j]+a[i][k]*b[k][j];  }  }  }  //display for(i=0;i<m;i++)  {  for(j=0;j<q;j++)  {  printf("%d\t",c[i][j]);  }  printf("\n");  } } 

